{
  "name": "visual-product-matcher-backend",
  "version": "1.0.0",
  "description": "Backend for visual product matcher using Node.js, Express, MongoDB, and Hugging Face",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed-products.js",
    "seed:quick": "node scripts/quick-seed.js",
    "seed:50plus": "node scripts/seed-50plus.js",
    "seed:api": "node scripts/seed-from-api.js",
    "seed:dummy": "node scripts/seed-from-dummyjson.js",
    "db:clear": "node scripts/clear-db.js",
    "db:reset": "node scripts/clear-db.js --confirm && node scripts/seed-products.js",
    "db:reset:50": "node scripts/clear-db.js --confirm && node scripts/seed-50plus.js",
    "db:reset:api": "node scripts/clear-db.js --confirm && node scripts/seed-from-api.js",
    "db:reset:dummy": "node scripts/clear-db.js --confirm && node scripts/seed-from-dummyjson.js"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "form-data": "^4.0.4",
    "jimp": "^1.6.0",
    "mime-types": "^3.0.1",
    "mongoose": "^8.5.3",
    "multer": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "huggingface",
    "image-similarity"
  ],
  "author": "",
  "license": "ISC"
}
